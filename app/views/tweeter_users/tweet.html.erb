<form action = "/tweeter_users/tweet" method ="post" enctype="multipart/form-data">
  <input type = "text" name="tweet_message" id = "tweet_message" required></input>
  <%= file_field_tag 'attachment' %>
  <%= submit_tag "Send Attachment" %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
</form>